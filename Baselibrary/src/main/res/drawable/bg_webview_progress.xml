            <gradient
                android:endColor="@color/yWhite"
                android:startColor="@color/yWhite" />
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <gradient
                    android:endColor="@color/yMain"
                    android:startColor="@color/yMain" />
            </shape>
        </clip>
    </item>
</layer-list>